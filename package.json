{
  "name": "qhh",
  "version": "0.3.1",
  "private": true,
  "watch": true,
  "engines": {
    "node": ">= 12.18.2",
    "pnpm": ">= 5.4.0"
  },
  "scripts": {
    "front-dev": "pnpm start --prefix ./front",
    "back-dev": "pnpm run dev --prefix ./back",
    "front": "docker-compose -f dockerfiles/front.yml --env-file \"./front/.env\" -p qhh_front up --build -d",
    "back": "docker-compose -f dockerfiles/back.yml --env-file \"./back/.env\" -p qhh_back up --build -d",
    "db": "docker-compose -f dockerfiles/mysql.yml --env-file \"./back/.env\" -p qhh_db up -d",
    "all": "pnpm run db && pnpm run back && pnpm run front",
    "stop-all": "docker-compose f dockerfiles/mysql.yml stop",
    "remove-all": "docker-compose f dockerfiles/mysql.yml rm"
  }
}
